<?xml version="1.0" encoding="utf-8"?>
<robot name="myfirst">

<material name="white">
    <color rgba="1 1 1 1"/>
  </material> 

<material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>

<material name="red">
  <color rgba="1 0 0 1"/>     
</material>

<material name="green">
  <color rgba="0 1 0 1"/>     
</material>

<material name="black">
  <color rgba="0 0 0 1"/>   
</material>

<material name="gray">
  <color rgba="0.5 0.5 0.5 1"/>  
</material>

<material name="yellow">
  <color rgba="1 1 0 1"/>     
</material>


<material name="silver">
  <color rgba="0.75 0.75 0.75 1"/>  
</material>

<material name="gold">
  <color rgba="1 0.84 0 1"/>   
</material>

<material name="orange">
  <color rgba="1 0.65 0 1"/>    
</material>

<material name="cyan">
  <color rgba="0 1 1 1"/>     
</material>

<material name="magenta">
  <color rgba="1 0 1 1"/>      
</material>


<material name="dark_green">
  <color rgba="0 0.5 0 1"/>    
</material>

<material name="navy_blue">
  <color rgba="0 0 0.5 1"/>    
</material>


<material name="glass">
  <color rgba="0.9 0.9 1 0.3"/>  
</material>


  <link name="base_link">
  </link>

  <link name="car_frame_link">
    <visual>
    <!-- <origin rpy="0 0 0" xyz="-0.29 -0.24 0.15"/> -->
    <origin rpy="0 0 0" xyz="-0.29 -0.24 0"/>
      <geometry>
        <mesh filename="package://my_urdf/models/base_frame.STL"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <link name="controller_base_link"> 
    <visual>
        <origin rpy="0 0 0" xyz="0.0 -0.1 0"/>
        <geometry>
          <mesh filename="package://my_urdf/models/jetson_base.STL"/>
        </geometry>
        <material name="white"/>
    </visual> 
  </link>

  <link name="imu_link"> 
    <visual>
        <origin rpy="0 0 0" xyz="0.0 0 0"/>
        <geometry>
          <box size="0.036 0.036 0.012"/>
        </geometry>
        <material name="blue"/>
    </visual> 
 </link>

  <link name="jetson_link"> 
    <visual>
        <origin rpy="0 0 0" xyz="0.0 0 0"/>
        <geometry>
          <box size="0.0787 0.1 0.04"/>
        </geometry>
        <material name="blue"/>
    </visual> 
  </link>

  <link name="gps_link"> 
    <visual>
        <origin rpy="0 0 0" xyz="0.0 0 0"/>
        <geometry>
          <box size="0.025 0.025 0.005"/>
        </geometry>
        <material name="blue"/>
    </visual> 
  </link>

  <link name="camera_link"> 
    <visual>
        <origin rpy="0 0 0" xyz="0.0 0 0"/>
        <geometry>
          <box size="0.025 0.1 0.02"/>
        </geometry>
        <material name="black"/>
    </visual> 
  </link>

  <link name="laser"> 
    <visual>
        <origin rpy="0 0 0" xyz="0.0 0 0"/>
        <geometry>
          <cylinder length="0.038" radius="0.038"/>
        </geometry>
        <material name="black"/>
    </visual> 
  </link>

  <link name="wheel1_link"> 
    <visual>
        <origin rpy="1.57075 0 0" xyz="0.0 0 0"/>
        <geometry>
          <cylinder length="0.05" radius="0.15"/>
        </geometry>
        <material name="black"/>
    </visual> 
  </link>

  <link name="wheel2_link"> 
    <visual>
        <origin rpy="1.57075 0 0" xyz="0.0 0 0"/>
        <geometry>
          <cylinder length="0.05" radius="0.15"/>
        </geometry>
        <material name="black"/>
    </visual> 
  </link>

  <link name="wheel3_link"> 
    <visual>
        <origin rpy="1.57075 0 0" xyz="0.0 0 0"/>
        <geometry>
          <cylinder length="0.05" radius="0.15"/>
        </geometry>
        <material name="black"/>
    </visual> 
  </link>

  <link name="wheel4_link"> 
    <visual>
        <origin rpy="1.57075 0 0" xyz="0.0 0 0"/>
        <geometry>
          <cylinder length="0.05" radius="0.15"/>
        </geometry>
        <material name="black"/>
    </visual> 
  </link>
  
  

  <joint name="base_link_to_frame" type="fixed">
    <parent link="base_link"/>
    <child link="car_frame_link"/>
    <origin xyz="-0.545 0.0 0.15"/>
  </joint>

  <joint name="car_frame_to_controller_base" type="fixed">
    <parent link="car_frame_link"/>
    <child link="controller_base_link"/>
    <origin xyz="0 0.0 0.02"/>
  </joint>

  <joint name="controller_base_to_jetson" type="fixed">
    <parent link="controller_base_link"/>
    <child link="jetson_link"/>
    <origin xyz="0.07 -0.023 0.02"/>
  </joint>

  <joint name="controller_base_to_imu" type="fixed">
    <parent link="controller_base_link"/>
    <child link="imu_link"/>
    <origin rpy="0 0 3.141593" xyz="0.073 0.062 0.006"/>
  </joint>

  <joint name="controller_base_to_gps" type="fixed">
    <parent link="controller_base_link"/>
    <child link="gps_link"/>
    <origin xyz="0.12 0.062 0.0025"/>
  </joint>

   <joint name="controller_base_to_camera" type="fixed">
    <parent link="controller_base_link"/>
    <child link="camera_link"/>
    <origin xyz="0.705 0 0.335"/>
  </joint>

  <joint name="controller_base_to_lidar" type="fixed">
    <parent link="controller_base_link"/>
    <child link="laser"/>
    <origin rpy="0 0 3.141593" xyz="1.275 0 0"/>
  </joint>
  
  <joint name="car_frame_to_wheel1" type="fixed">
    <parent link="car_frame_link"/>
    <child link="wheel1_link"/>
    <origin xyz="0.97 0.285 0"/>
  </joint>

  <joint name="car_frame_to_wheel2" type="fixed">
    <parent link="car_frame_link"/>
    <child link="wheel2_link"/>
    <origin xyz="0.97 -0.285 0"/>
  </joint>

  <joint name="car_frame_to_wheel3" type="fixed">
    <parent link="car_frame_link"/>
    <child link="wheel3_link"/>
    <origin xyz="-0.28 0.285 0"/>
  </joint>

  <joint name="car_frame_to_wheel4" type="fixed">
    <parent link="car_frame_link"/>
    <child link="wheel4_link"/>
    <origin xyz="-0.28 -0.285 0"/>
  </joint>

</robot>